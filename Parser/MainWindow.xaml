<Window x:Name="HabraParser" x:Class="Parser.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Parser"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <ListBox x:Name="ListTitles" Margin="10,10,253,10"/>
        <Label Content="Start Point" HorizontalAlignment="Left" Margin="596,95,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="NumericStart" HorizontalAlignment="Left" Margin="596,121,0,0" Text="1" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Cursor="" InputScope="Number"/>
        <Label Content="End Point" HorizontalAlignment="Left" Margin="596,146,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="NumericEnd" HorizontalAlignment="Left" Margin="596,177,0,0" Text="2" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Cursor="" InputScope="Number"/>
        <Button Content="Start" HorizontalAlignment="Left" Margin="616,218,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
        <Button Content="Abort" HorizontalAlignment="Left" Margin="615,251,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_1"/>

    </Grid>
</Window>
